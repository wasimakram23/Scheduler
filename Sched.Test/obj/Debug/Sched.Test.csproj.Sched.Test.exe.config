<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!--Error Logs-->
    <add key="ErrorLogDirectory" value="C:\\InstantCommissionPayment\\Errors" />
    <add key="JobDurationMilliseconds" value="5000" />
    <add key="CronJobExpression" value="0/45 0-59 0-23 * * ?" />
    <!--12 AM to 11 PM every 45 second-->
    <!--<add key="CronJobExpression" value="0 30 2 1/1 * ? *" />  -->
    <!--Every hour at 20,40-->
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Common.Logging" publicKeyToken="af08829b84f0328e" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.4.1.0" newVersion="3.4.1.0" />
      </dependentAssembly>
    </assemblyBinding>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Common.Logging.Core" publicKeyToken="af08829b84f0328e" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.4.1.0" newVersion="3.4.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>